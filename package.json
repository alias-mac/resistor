{
  "name": "resistor",
  "version": "1.0.0",
  "description": "Resistor Calculator",
  "repository": {
    "url": "git@github.com:alias-mac/resistor.git",
    "type": "git"
  },
  "author": "Filipe Guerra <alias.mac@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.22.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "bootstrap": "4.0.0-alpha.5",
    "coveralls": "^2.11.15",
    "css-loader": "^0.28.4",
    "debug": "^3.1.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "enzyme-to-json": "^3.0.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "gh-pages": "^1.0.0",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.5",
    "prop-types": "^15.7.2",
    "raf": "^3.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.3.0"
  },
  "scripts": {
    "build": "npm-run-all -l -p build:*",
    "build:server": "babel-node src/server.js",
    "build:client": "webpack --color --config .config/webpack.config.js -p",
    "deploy": "gh-pages -d public",
    "start": "npm-run-all -l -p -r start:*",
    "start:server": "nodemon src/server.js --watch src/ --exec babel-node",
    "start:client": "webpack-dev-server --color --progress --inline --hot --watch-content-base --config .config/webpack.config.js",
    "lint": "eslint .",
    "test": "jest"
  },
  "jest": {
    "rootDir": "src",
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
